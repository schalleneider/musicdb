{
    "log": {
        "level": "trace"
    },
    "database" : {
        "path" : "database/template.example.sqlite3"
    },
    "sources": {
        "jpop": {
            "table": "JPop",
            "limit": "LIMIT 10"
        },
        "drama": {
            "table": "Drama",
            "limit": "LIMIT 10"
        },
        "games": {
            "table": "Games",
            "limit": "LIMIT 10"
        },
        "vocaloid": {
            "table": "Vocaloid",
            "limit": "LIMIT 10"
        },
        "doujin": {
            "table": "Doujin",
            "limit": "LIMIT 10"
        },
        "animation": {
            "table": "Animation",
            "limit": "LIMIT 10"
        },
        "anime": {
            "table": "Anime",
            "limit": "LIMIT 10"
        },
        "ost": {
            "table": "OST",
            "limit": "LIMIT 10"
        }
    },
    "command": {
        "download": {
            "delay": 100,
            "status": "READY_TO_DOWNLOAD",
            "binPath": "bin/",
            "outputPath": "/musicdb-out/",
            "mediaFormat": "bestaudio/best",
            "audioQuality": 0,
            "audioFormat": "m4a"
        },
        "tags": {
            "delay": 100,
            "status": "DONE",
            "binPath": "bin/",
            "outputPath": "/musicdb-out/",
            "audioFormat": "m4a"
        },
        "filename" : {
            "delay": 100,
            "status": "FILENAME_FIX",
            "searchPath": "/musicdb-out/filename/"
        }
    }
}